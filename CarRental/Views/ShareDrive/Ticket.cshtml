@using CarRental.Models.ShareDrive
@model Ticket

<h2>Your Tickets</h2>
<embed src="@Url.Content("~/tickets/" + Model.TicketID + ".pdf")" width="100%" height="500px" type="application/pdf" />

<!-- Display QR Code -->
<h3>QR Code for your ticket</h3>
<img src="data:image/png;base64,@Model.QRCode" alt="QR Code" width="250" height="250" />
